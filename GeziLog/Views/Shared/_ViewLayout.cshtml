
@{
    Layout = null;
}


<!doctype html>
<html lang="tr">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <title>GeziLog</title>

    <!-- Template CSS -->
    <link rel="stylesheet" href="~/web/assets/css/style-starter.css">
    <link rel="stylesheet" href="~/web2/css/style.css">
</head>

<body>
    <!--header-->
    <header id="site-header">
        <div class="container">
            <nav class="navbar navbar-expand-lg navbar-dark stroke p-0">
                <h1>
                    <a class="navbar-brand" href="/Default/Index/">
